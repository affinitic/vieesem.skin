<?xml version="1.0"?>
<object name="portal_skins"
        allow_any="False"
        cookie_persistence="False"
        default_skin="Theme package for VIEESM website">

    <object name="vieesm_skin_images"
            meta_type="Filesystem Directory View"
            directory="vieesm.skin:skins/vieesm_skin_images"/>
    <object name="vieesm_skin_style"
            meta_type="Filesystem Directory View"
            directory="vieesm.skin:skins/vieesm_skin_style"/>

    <skin-path name="Theme package for VIEESM website"
               based-on="Sunburst Theme">
        <layer name="vieesm_skin_images"
               insert-after="custom"/>
        <layer name="vieesm_skin_style"
               insert-after="custom"/>

    </skin-path>

</object>
